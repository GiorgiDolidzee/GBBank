<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.GBBank" parent="Theme.MaterialComponents.DayNight.DarkActionBar">


        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/application_primary_color</item>
        <item name="colorOnPrimary">@color/white</item>


        <!-- Secondary brand color. -->
        <item name="colorOnSecondary">@color/black</item>


        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/backgroundColor</item>
        <item name="android:windowLightStatusBar">true</item>

        <item name="android:navigationBarColor">?attr/white_black</item>
        <item name="android:windowLightNavigationBar">true</item>


        <!-- Custom Colors -->
        <item name="applicationPrimaryColor">@color/application_primary_color</item>

        <item name="backgroundColor">@color/background_color</item>
        <item name="secondaryBackgroundColor">@color/secondary_background_color</item>
        <item name="iconColor">@color/icon_color</item>
        <item name="primaryTextColor">@color/primary_text_color</item>
        <item name="secondaryTextColor">@color/secondary_text_color</item>

        <item name="white_black">@color/white</item>
        <item name="black_white">@color/black</item>

        <item name="logo">@drawable/logo_light_mode</item>

    </style>
</resources>